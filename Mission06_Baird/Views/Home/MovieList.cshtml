@{
}
@using Mission06_Baird.Models 

@model List<Mission06_Baird.Models.MovieSubmission>


<table>

    <thead>
        <td>Category</td>
        <td>Title</td>
        <td>Year</td>
        <td>Director</td>
        <td>Rating</td>
        <td>Edited</td>
        <td>LentTo</td>
        <td>Notes</td>
    </thead>


    @foreach (var x in Model)
    {
        <tbody>
            <td>@x.Category</td>
            <td>@x.Title</td>
            <td>@x.Year</td>
            <td>@x.Director</td>
            <td>@x.Rating</td>
            <td>@x.Edited</td>
            <td>@x.LentTo</td>
            <td>@x.Notes</td>
        </tbody> 
    }

</table>